{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.js",
            "methods": ["GET"],
            "headers": {
                "Access-Control-Allow-Origin": "https://task5-react-front.vercel.app"
            }
        }
    ]
}
